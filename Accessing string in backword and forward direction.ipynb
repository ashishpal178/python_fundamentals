    "#Accessing string in backword and forward direction\n",
    "s=input('Enter some String:')\n",
    "n=len(s)\n",
    "m=-n\n",
    "i=0\n",
    "j=-1\n",
    "print('Data in Forward Direction')\n",
    "while i<n:\n",
    "    print(s[i],end='')\n",
    "    i=i+1\n",
    "print('')\n",
    "print('Data in Backward Direction')\n",
    "while j>=m:\n",
    "    print(s[j],end='')\n",
    "    j=j-1"
   ]
